extends layout

block content
  h1= title
  p= msg
  form(action="send", 
      method="post",
      enctype="multipart/form-data")
   p
      input(type="text", name="name", required)
   p
      input(type="text", name="email", required)
   p
      input(type="text", name="phone", required)
   p
   p
      input(name="cv",
            type="file",
            required)
   
   p
      button(type="submit") Submit

   
